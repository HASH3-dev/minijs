{
  "name": "@mini/core",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --sourcemap && npm run build:jsx-runtime",
    "build:jsx-runtime": "node -e \"require('fs').writeFileSync('dist/jsx-runtime.js', 'export * from \\\"./index.js\\\";\\n'); require('fs').writeFileSync('dist/jsx-runtime.cjs', 'module.exports = require(\\\"./index.cjs\\\");\\n'); require('fs').writeFileSync('dist/jsx-runtime.d.ts', 'export * from \\\"./index.js\\\";\\n');\"",
    "test": "vitest run"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  }
}
