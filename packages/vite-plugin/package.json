{
  "name": "@mini/vite-plugin",
  "version": "0.1.0",
  "description": "Vite plugin for Mini Framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --sourcemap",
    "dev": "tsup src/index.ts --dts --format esm --sourcemap --watch"
  },
  "keywords": [
    "vite",
    "plugin",
    "mini-framework",
    "jsx"
  ],
  "peerDependencies": {
    "vite": "^7.2.2"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
