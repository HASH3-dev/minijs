{
  "name": "@mini/vite-plugin",
  "version": "0.1.0",
  "description": "Vite plugin for Mini Framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/lazyTransform.ts --dts --format esm --sourcemap",
    "dev": "tsup src/index.ts src/lazyTransform.ts --dts --format esm --sourcemap --watch"
  },
  "keywords": [
    "vite",
    "plugin",
    "mini-framework",
    "jsx"
  ],
  "peerDependencies": {
    "vite": "^7.2.2"
  },
  "dependencies": {
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.26.4",
    "@babel/generator": "^7.26.2",
    "@babel/types": "^7.26.0"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.5",
    "@types/babel__traverse": "^7.20.6",
    "@types/babel__generator": "^7.6.8",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
